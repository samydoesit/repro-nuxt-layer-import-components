{
  "name": "monorepo-vue-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo pnpm --filter nuxt-app run build",
    "dev": "turbo run dev --filter nuxt-app",
    "start:nuxt-app": "pnpm run start --filter nuxt-app",
    "start": "pnpm -r run start",
    "typecheck": "pnpm -r run typecheck"
  },
  "devDependencies": {
    "typescript": "5.3.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@8.10.5",
  "dependencies": {
    "turbo": "^1.10.16"
  }
}
